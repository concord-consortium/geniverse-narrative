<?xml version="1.0" encoding="utf-8"?>
<root>
    <sequence id="sequence-1">
        
        <!-- Animated PNG sequences: Skipping Stones -->
        <behavior panel="1" selector="div#layerC div.rowId1 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="1"/>
            <frame index="3" timeIn="1.3"/>
            <frame index="4" timeIn="1.4"/>
            <frame index="5" timeIn="1.5"/>
            <frame index="6" timeIn="1.6"/>
            <frame index="7" timeIn="1.7"/>
            <frame index="8" timeIn="1.8"/>
            <frame index="9" timeIn="1.9"/>
            <frame index="10" timeIn="2.0"/>
            <frame index="11" timeIn="2.1"/>
            <frame index="12" timeIn="2.4"/>
            <frame index="13" timeIn="3.4"/>
            <frame index="14" timeIn="3.7"/>
            <frame index="15" timeIn="3.9"/>
        </behavior>
        
        <!-- Animated PNG sequences: Arrow pull -->
        <behavior panel="5" selector="div#layerD div.rowId5 img.image-sequence" time="0" type="animated-png" loop="false">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="2.2"/>
            <frame index="3" timeIn="2.4"/>
            <frame index="4" timeIn="2.6"/>
            <frame index="5" timeIn="2.8"/>
            <frame index="6" timeIn="3.0"/>
            <frame index="7" timeIn="3.2"/>
            <frame index="8" timeIn="3.4"/>
        </behavior>
        
        <!-- Scene 2: Dragon fly in -->
        <behavior panel="2" selector="div#layerB div.rowId2 img#dragon" time="1" type="animation" duration="1.25" mask="true" ease="easeOutQuad">
            <prop name="top" start="409" end="0"/>
            <prop name="left" start="350" end="80"/>
            <prop name="transform" start="scale(0.1,0.1)" end="scale(1,1)"/>
        </behavior>
        
        <!-- Scene 3: hunters over the hill -->
        <behavior panel="3" selector="div#layerB div.rowId3 img#hunters" time="1" type="animation" duration="2" mask="true" ease="easeOutQuad">
            <prop name="top" start="209" end="7"/>
            <prop name="left" start="300" end="0"/>
            <prop name="transform" start="scale(0.2,0.2)" end="scale(.9,.9)"/>
        </behavior>
        
        <!-- Scene 3: scarlett reaction to poachers -->
        <behavior panel="3" selector="div#layerD div.rowId3 img#layerD-rowId3-scarlett" time="3" type="animation" duration="1" ease="easeOutQuad">
            <prop name="left" start="0" end="-50"/>
        </behavior>
        
        <!-- Scene 4: poachers rise -->
        <behavior panel="4" selector="div#layerD div.rowId4 img#layerD-rowId4-poachers" time="1" type="animation" duration="1" mask="true" ease="easeOutQuad">
            <prop name="top" start="409" end="0"/>
        </behavior>
        
        <!-- Scene 5: small hunters descending -->
        <behavior panel="5" selector="div#layerB div.rowId5 img#rowId5-hunters" time="1" type="animation" duration="4" ease="easeOutQuad">
            <prop name="top" start="90" end="190"/>
            <prop name="left" start="520" end="650"/>
        </behavior>
        
        <!-- Audio Clips -->
        <audio panel="1" src="audio/sequence-1/scene-1.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="2" src="audio/sequence-1/scene-2.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="2" src="audio/sequence-1/arrow.mp3" volume="1" delay="1.25" pan="0" fadeOut="0.75" loops="0"/>
        <audio panel="3" src="audio/sequence-1/scene-3.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="4" src="audio/sequence-1/scene-4.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="5" src="audio/sequence-1/scene-5.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="6" src="audio/sequence-1/scene-6.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        
    </sequence> 
    <sequence id="sequence-2">
        
        <!-- Animated PNG sequences: Pumpkin roll -->
        <behavior panel="1" selector="div#layerD div.rowId1 img.image-sequence" time="0" type="animated-png" loop="false">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="2"/>
            <frame index="3" timeIn="2.2"/>
            <frame index="4" timeIn="2.4"/>
            <frame index="5" timeIn="2.6"/>
            <frame index="6" timeIn="2.8"/>
            <frame index="7" timeIn="3.0"/>
            <frame index="8" timeIn="5.0"/>
            <frame index="9" timeIn="5.5"/>
        </behavior>
        
        <!-- Animated PNG sequences:  Steam -->
        <behavior panel="2" selector="div#layerC div.rowId2 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
        </behavior>
        <behavior panel="2" selector="div#layerD div.rowId2 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
        </behavior>
        
        <!-- Animated PNG sequences:  Windmill turning -->
        <behavior panel="3" selector="div#layerB div.rowId3 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
            <frame index="5" timeIn="0.8"/>
        </behavior>
        
        <!-- Animated PNG sequences: Girl waving -->
        <behavior panel="3" selector="div#layerC div.rowId3 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
        </behavior>
        
        <!-- Animated PNG sequences:  Rain falling -->
        <behavior panel="3" selector="div#layerD div.rowId3 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
        </behavior>
        
        <!-- Animated PNG sequences:  Dragon breathing -->
        <behavior panel="5" selector="div#layerD div.rowId5 img.image-sequence" time="0" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
        </behavior>
        
        
        <!-- Animated PNG sequences:  Dragon Flying-->
        <behavior panel="6" selector="div#layerD div.rowId6 img.image-sequence" time="1" type="animated-png" loop="true">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="0.2"/>
            <frame index="3" timeIn="0.4"/>
            <frame index="4" timeIn="0.6"/>
            <frame index="5" timeIn="0.8"/>
            <frame index="6" timeIn="1.0"/>
            <frame index="7" timeIn="1.2"/>
            <frame index="8" timeIn="1.4"/>
            <frame index="9" timeIn="1.6"/>
            <frame index="-1" timeIn="1.8"/>
            <frame index="-1" timeIn="3.8"/>
        </behavior>
        
        
        <!-- Scene 2: Drake rises, Character slides in -->
        <behavior panel="2" selector="div#layerB div.rowId2 img#layerB-rowId2-drake" time="4" type="animation" duration="1" mask="true" maskIndex="1" ease="easeOutQuad">
            <prop name="top" start="409" end="0"/>
            <prop name="left" start="58" end="58"/>
        </behavior>
        <behavior panel="2" selector="div#layerD div.rowId2 img#scene-2-character" time="1" type="animation" duration="3" mask="false" ease="easeOutQuad">
            <prop name="left" start="-158" end="0"/>
        </behavior>
        
        <!-- Scene 4: Axe fly in, overlap positioning, scarlett flee, poachers rise -->
        <!-- note axe rotation is in radians... so this is  4 rotations * 2Ï€ radians = 8 * 3.141592654etc = 25.133 -->
        <behavior panel="4" selector="div#layerC div.rowId4 img#layerC-rowId4-axe" time="1" type="animation" duration="1.25" mask="true" maskIndex="1" ease="easeInQuad">
            <prop name="left" start="-55" end="643"/>
            <prop name="top" start="409" end="105"/>
            <prop name="transform" start="rotate(0)" end="rotate(25.133)"/> 
        </behavior>
        <behavior panel="4" selector="div#layerC div.rowId4 img#layerC-rowId4-overlap" time="2" type="animation" duration="1" ease="easeOutQuad">
            <prop name="left" start="20" end="20"/>
        </behavior>
        <behavior panel="4" selector="div#layerC div.rowId4 img#layerC-rowId4-scarlett" time="2" type="animation" duration="1.5" mask="true" ease="easeOutQuad">
            <prop name="left" start="0" end="195"/>
            <prop name="top" start="0" end="175"/>
            <prop name="transform" start="scale(1,1)" end="scale(0.5,0.5)"/>
        </behavior>
        <behavior panel="4" selector="div#layerD div.rowId4 img#layerD-rowId4-poachers" time="2.25" type="animation" duration="1" mask="true" ease="easeOutQuad">
            <prop name="left" start="-1024" end="0"/>
            <prop name="top" start="409" end="0"/>
        </behavior>
        
        <!-- Scene 6: Foreground drops, characters rise -->
        <behavior panel="6" selector="div#layerD div.rowId6 img#scene-bottom" time="2" type="animation" duration="3" mask="true" maskIndex="1" ease="easeOutQuad">
            <prop name="top" start="0" end="100"/>
        </behavior>
        <behavior panel="6" selector="div#layerD div.rowId6 img#scene-6-character" time="2" type="animation" duration="3" mask="true" maskIndex="2" ease="easeOutQuad">
            <prop name="left" start="-75" end="-10"/>
            <prop name="top" start="100" end="0"/>
        </behavior>
        
        <!-- Audio Clips -->
        <audio panel="1" src="audio/sequence-2/scene-1.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="2" src="audio/sequence-2/scene-2.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="3" src="audio/sequence-2/scene-3.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="4" src="audio/sequence-2/scene-4.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="5" src="audio/sequence-2/scene-5.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="6" src="audio/sequence-2/scene-6.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        
    </sequence> 
    <sequence id="sequence-3">
        
        <!-- Animated PNG sequences: Dr Weaver turns away -->
        <behavior panel="1" selector="div#layerD div.rowId1 img.image-sequence" time="0" type="animated-png" loop="false">
            <frame index="1" timeIn="0"/>
            <frame index="2" timeIn="3"/>
        </behavior>
        
        <behavior panel="2" selector="div#layerB div.rowId2 img#scarlett" maskIndex="2" time="2" type="animation" duration="3" mask="true" ease="easeOutQuad">
            <prop name="left" start="706" end="0"/>
        </behavior>
        <behavior panel="2" selector="div#layerB div.rowId2 img#arrow" maskIndex="1" time="1" type="animation" duration="5" mask="true" ease="easeOutQuad">
            <prop name="left" start="706" end="0"/>
        </behavior>
        
        <audio panel="1" src="audio/sequence-3/scene-1.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="2" src="audio/sequence-3/scene-2.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        <audio panel="3" src="audio/sequence-3/scene-3.mp3" volume="1" delay="0" pan="0" fadeOut="0.75" loops="100"/>
        
    </sequence> 
</root>
